{
  "name": "ml5-fluid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "ml5": "^0.3.1",
    "p5": "^0.9.0",
    "parcel-bundler": "^1.12.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "standard": "^13.0.0"
  },
  "scripts": {
    "dev": "yarn rm:dev && parcel index.html",
    "build": "yarn rm:build && parcel build src/*.html -d build/ --no-content-hash --no-source-maps",
    "rm:dev": "rm -rf dist/ && yarn rm:cache",
    "rm:build": "rm -rf build/ && yarn rm:cache",
    "rm:cache": "rm -rf .cache"
  },
  "standard": {
    "ignore": [
      "./fluid.js"
    ]
  }
}
